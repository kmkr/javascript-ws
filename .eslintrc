// http://eslint.org/docs/user-guide/configuring
{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "restParams": true,
    "spread": true,
    "templateStrings": true
  },
  "rules": {
    "curly": 2,
    "comma-dangle": 2,
    "default-case": 2,
    "dot-notation": 1,
    "default-case": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "indent": [2, 2],
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-eval": 2,
    "no-extend-native": 1,
    "no-extra-bind": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-irregular-whitespace": 2,
    "no-invalid-regexp": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-loop-func": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-nested-ternary": 1,
    "no-native-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-self-compare": 2,
    "no-undef": 2,
    "no-shadow-restricted-names": 2,
    "no-unused-vars": 2,
    "no-unreachable": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "quotes": [2, "single"],
    "radix": 2,
    "valid-typeof": 2,
    "wrap-iife": 2,
    "semi": [2, "always"]
  },
  "globals": {
    "before": false,
    "beforeAll": false,
    "beforeEach": false,
    "afterEach": false,
    "afterAll": false,
    "describe": false,
    "export": false,
    "it": false,
    "expect": false,
    "spyOn": false,
    "module": false,
    "require": false,
    "import": false,
    "exports": false,
    "__dirname": false,
    "jasmine": false,
    "window": false
  }
}
  
